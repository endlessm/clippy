{
  "id": "com.endlessm.Clippy",
  "branch": "eos3",
  "runtime": "com.endlessm.apps.Platform",
  "sdk": "com.endlessm.apps.Sdk",
  "runtime-version": "5",
  "appstream-compose": false,
  "separate-locales": false,
  "command": "clippy-test",
  "modules": [
    {
      "name": "clippy",
      "buildsystem": "meson",
      "config-opts": [
        "-Dgtk-modules-path=/app/lib/"
      ],
      "sources": [
        {
          "type": "dir",
          "path": "."
        }
      ]
    },
    {
      "name": "clippy-bin",
      "buildsystem": "simple",
      "sources": [
        {
          "type": "file",
          "path": "clippy-test"
        }
      ],
      "build-commands": [
        "install -d /app/bin",
        "install clippy-test /app/bin/clippy-test"
      ]
    }
  ]
}
